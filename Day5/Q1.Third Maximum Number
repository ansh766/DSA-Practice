class Solution {
public:
    int thirdMax(vector<int>& nums) {
        int n = nums.size();
        int  first = INT_MIN,second = INT_MIN,third = INT_MIN;
        int count = 0;
        bool flag = 0;
    for(int i = 0; i<n; i++){
         if(nums[i]>first){
            third = second;
            second = first;
            first = nums[i];
            count++;
         }
         else if(nums[i]>second&&nums[i]<first){
            third = second;
            second = nums[i];
            count++;
         }
         else if(nums[i]==INT_MIN&&flag==0||nums[i]>third&&nums[i]<first
         &&nums[i]<second){
             third = nums[i];
             count++;
             flag = 1;
         }

    }
    if(count<3){
         return first;
    }
    return third;


    }
};
